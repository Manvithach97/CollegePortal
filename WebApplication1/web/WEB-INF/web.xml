<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Loginservlet</servlet-name>
        <servlet-class>Loginservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signupservelet</servlet-name>
        <servlet-class>Signupservelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResetPasswordServlet</servlet-name>
        <servlet-class>ResetPasswordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reset1</servlet-name>
        <servlet-class>reset1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>emsignup</servlet-name>
        <servlet-class>emsignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>elogin</servlet-name>
        <servlet-class>elogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmpServlet</servlet-name>
        <servlet-class>EmpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Loginservlet</servlet-name>
        <url-pattern>/Loginservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signupservelet</servlet-name>
        <url-pattern>/Signupservelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResetPasswordServlet</servlet-name>
        <url-pattern>/ResetPasswordServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reset1</servlet-name>
        <url-pattern>/reset1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>emsignup</servlet-name>
        <url-pattern>/emsignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>elogin</servlet-name>
        <url-pattern>/elogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmpServlet</servlet-name>
        <url-pattern>/EmpServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
